{
  "name": "appcket-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.config.json",
    "test:watch": "jest --watch --config ./test/jest.config.json",
    "test:cov": "jest --coverage --config ./test/jest.config.json",
    "test:debug": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest.config.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.9.4",
    "@mikro-orm/nestjs": "^5.2.2",
    "@mikro-orm/postgresql": "^5.9.4",
    "@nestjs/apollo": "^12.0.11",
    "@apollo/server": "^4.9.5",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.2.10",
    "apollo-server-express": "^3.13.0",
    "ca-append": "^0.1.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "keycloak-connect": "^22.0.5",
    "json-diff-ts": "^1.2.6",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.5.0",
    "pino-http": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "jest-when": "^3.6.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
