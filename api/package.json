{
  "name": "appcket-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.config.json",
    "test:watch": "jest --watch --config ./test/jest.config.json",
    "test:cov": "jest --coverage --config ./test/jest.config.json",
    "test:debug": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest.config.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.2.2",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.2.2",
    "@nestjs/apollo": "^12.1.0",
    "@apollo/server": "^4.10.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.8",
    "apollo-server-express": "^3.13.0",
    "ca-append": "^0.1.0",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "express-session": "^1.18.0",
    "graphql": "^16.8.1",
    "keycloak-connect": "^24.0.3",
    "json-diff-ts": "^1.2.6",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.0.0",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-when": "^3.6.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
